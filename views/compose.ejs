<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="mb-3">
    <label for="title" class="form-label">Title</label>
    <input name="titleBody" class="form-control" type="text" required autocomplete="off">
    </div>
    <div class="mb-3">
    <label for="post" class="form-label">Post</label>
    <textarea name="postBody" class="form-control" id="post" cols="30" rows="5" required autocomplete="off"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>